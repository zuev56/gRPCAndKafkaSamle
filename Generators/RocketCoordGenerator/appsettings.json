{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "FormatterName": "SingleLine",
      "FormatterOptions": {
        "SingleLine": true
      }
    }
  },

  "AllowedHosts": "*",

  "Kestrel": {
    "Endpoints": {
      "WebAPI": {
        "Url": "http://localhost:7010",
        "Protocols": "Http1"
      }
    }
  },

  "Kafka": {
    "BootstrapServers": "192.168.1.104:9092",
    "Topic": "coordinates"
  }
}